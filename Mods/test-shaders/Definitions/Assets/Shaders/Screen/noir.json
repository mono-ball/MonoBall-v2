  "sourceFile": "Shaders/Noir.mgfxo",
  "parameters": [
    { "name": "Time", "type": "float", "defaultValue": 0.0 },
    { "name": "Contrast", "type": "float", "defaultValue": 1.8, "min": 1.0, "max": 3.0 },
    { "name": "Brightness", "type": "float", "defaultValue": -0.1, "min": -0.5, "max": 0.5 },
    { "name": "VignetteIntensity", "type": "float", "defaultValue": 0.7, "min": 0.0, "max": 1.5 },
    { "name": "GrainAmount", "type": "float", "defaultValue": 0.08, "min": 0.0, "max": 0.2 },
    { "name": "ShadowTint", "type": "float3", "defaultValue": [0.1, 0.1, 0.15] },
    { "name": "HighlightTint", "type": "float3", "defaultValue": [1.0, 0.98, 0.95] },
    { "name": "ScreenSize", "type": "float2", "defaultValue": [1280.0, 720.0] }
  ]
}
